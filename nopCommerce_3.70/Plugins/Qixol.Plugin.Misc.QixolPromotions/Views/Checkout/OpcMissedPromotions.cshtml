@model MissedPromotionsModel
@using Nop.Web.Models.Checkout;
<div class="checkout-data">
    @Html.Widget("op_checkout_missed_promotions_top")
    <div class="section">
        @Html.Partial("MissedPromotionsSummary", Model)
    </div>
    @Html.Widget("op_checkout_missed_promotions_bottom")
</div>
@if (Model.MissedPromotions.Count == 0)
{
    <script type="text/javascript">
        $(document).ready(function () {
            MissedPromotions.next();
        });
    </script>
}